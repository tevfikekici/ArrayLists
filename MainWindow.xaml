<Window x:Class="ArrayLists.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ArrayLists"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <ListBox x:Name="listBox" HorizontalAlignment="Left" Height="345" Margin="10,40,0,0" VerticalAlignment="Top" Width="125"/>
        <Button x:Name="button" Content="Fill" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="75" Click="button_Click"/>
        <Button x:Name="button_Sort" Content="Sort" HorizontalAlignment="Left" Margin="101,10,0,0" VerticalAlignment="Top" Width="75" Click="button_Sort_Click"/>
        <ListBox x:Name="listBox_1" HorizontalAlignment="Left" Height="345" Margin="140,40,0,0" VerticalAlignment="Top" Width="125"/>
        <Button x:Name="button_Range" Content="Range" HorizontalAlignment="Left" Margin="189,10,0,0" VerticalAlignment="Top" Width="75" Click="button_Range_Click"/>

    </Grid>
</Window>
